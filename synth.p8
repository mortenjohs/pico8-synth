version 34
__lua__
vol = 5
root = 36 // c 3

function _init()
 a = 0
end

function _update60()
 out_string = ""
 for i=0,5 do
  if btn(i) then
			note = root+scale["scale"][i+1]
  	play(form["form"],note,vol,"wave"..i)
  	out_string = out_string..note_to_s(note).." "
  else
  	set_vol(o,"wave"..i)   
  end
 end
 

// https://www.lexaloffle.com/bbs/?tid=29382

do
  local state={}
  function play(form,freq,vol,ch)
    local id=state[ch]
    if not id then
      id=flr(rnd(4))
      for k,v in pairs(state) do
        if v==id then state[k]=nil end
      end
    end 
    state[ch]=id
    local n=63-id
    local p=0x3200+68*n
    poke(p,band(freq,63)+band(form,3)*64)
    poke(p+1,band(vol,7)*2+band(form,4)/4)
    poke2(p+66,256)
    sfx(n,id)
  end
  
  function set_vol(vol,ch)
    local id=state[ch]
    if id then
	    state[ch]=id
 	   local n=63-id
  	  local p=0x3200+68*n
    	poke(p+1,band(vol,7)*2+band(form,4)/4)
    	poke2(p+66,256)
    	sfx(n,id)
    end
  end
end
